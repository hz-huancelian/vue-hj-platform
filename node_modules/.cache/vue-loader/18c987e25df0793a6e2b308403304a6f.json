            <el-button v-loading.fullscreen.lock=\"outStockLoading\" type=\"primary\" @click=\"handleBatchOutStock\"> 批量出库</el-button>\n          </el-form-item>\n        </el-col>\n        <el-col :span=\"4\" class=\"search-right\">\n          <el-button type=\"text\" icon=\"el-icon-refresh\" @click=\"handleSearch\">刷新</el-button>\n        </el-col>\n      </el-row>\n    </el-form>\n    <div>\n      <el-table\n        stripe\n        border\n        :data=\"stockData\"\n        highlight-current-row\n        :header-cell-style=\"{background:'#ECF3FC'}\"\n        style=\"width: 100%\"\n        @selection-change=\"handleSelectionChange\"\n      >\n        <template slot=\"empty\">\n          <div class=\"table_empty\">\n            <div class=\"empty_tips\">\n              <span>\n                暂无审核信息\n              </span>\n            </div>\n          </div>\n        </template>\n        <el-table-column type=\"selection\" width=\"50\" align=\"center\" />\n        <el-table-column type=\"index\" width=\"50\" label=\"序号\" fixed=\"left\" align=\"center\" />\n        <el-table-column prop=\"sampleNo\" label=\"样品编号\" fixed=\"left\" min-width=\"120px\" align=\"center\" />\n        <el-table-column prop=\"jobId\" label=\"任务单号\" width=\"130px\" align=\"center\" />\n        <el-table-column prop=\"projectName\" label=\"项目名称\" width=\"150px\" align=\"center\" />\n        <el-table-column prop=\"secdClassName\" label=\"检测类别\" align=\"center\" />\n        <el-table-column prop=\"factorPoint\" label=\"采样点位\" align=\"center\" />\n        <el-table-column prop=\"inspectionName\" label=\"受检单位\" align=\"center\" />\n        <el-table-column prop=\"collectUser\" label=\"采样员\" width=\"100px\" align=\"center\" />\n        <el-table-column prop=\"reviewUser\" label=\"复核人\" width=\"100px\" align=\"center\" />\n        <el-table-column label=\"样品状态\" align=\"center\">\n          <template>\n            <el-tag type=\"\">待出库</el-tag>\n          </template>\n        </el-table-column>\n        <el-table-column fixed=\"right\" align=\"center\" label=\"操作\" width=\"100\">\n          <template slot-scope=\"scope\">\n            <el-button type=\"text\" @click=\"handleDetail(scope.row)\"> 查看</el-button>\n            <div v-if=\"scope.row.collectUser && scope.row.collectUser.length > 0\" class=\"erc-divider-vertical\" />\n            <el-button v-if=\"scope.row.collectUser && scope.row.collectUser.length > 0\" v-loading.fullscreen.lock=\"outStockLoading\" type=\"text\" @click=\"handleOutStock(scope.row)\"> 出库</el-button>\n          </template>\n        </el-table-column>\n      </el-table>\n    </div>\n    <div style=\"float: right\">\n      <pagination\n        v-show=\"total>0\"\n        :total=\"total\"\n        :page.sync=\"searchForm.pageNumber\"\n        :limit.sync=\"searchForm.pageSize\"\n        @pagination=\"handleSearch\"\n      />\n    </div>\n\n    <el-dialog :title=\"currentSample + '-详情'\" :visible.sync=\"sampleVisible\" width=\"80%\" center>\n      <el-form :model=\"sampleForm\" label-width=\"90px\">\n        <el-divider>样品基础信息</el-divider>\n        <el-row>\n          <el-col :span=\"8\">\n            <el-form-item label=\"检测因子:\">\n              <el-input v-model=\"sampleForm.factorName\" style=\"width:95%\" disabled />\n            </el-form-item>\n          </el-col>\n          <el-col :span=\"8\">\n            <el-form-item label=\"样品性状:\">\n              <el-input v-model=\"sampleForm.sampleProperties\" style=\"width:95%\" disabled />\n            </el-form-item>\n          </el-col>\n          <el-col :span=\"8\">\n            <el-form-item label=\"样品固定剂:\">\n              <el-input v-model=\"sampleForm.sampleFixative\" style=\"width:95%\" disabled />\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :span=\"8\">\n            <el-form-item label=\"录样位置:\">\n              <el-input v-model=\"sampleForm.collectLocation\" style=\"width:95%\" disabled />\n            </el-form-item>\n          </el-col>\n          <el-col :span=\"8\">\n            <el-form-item label=\"采样时间:\">\n              <el-input v-model=\"sampleForm.collectTime\" style=\"width:95%\" disabled />\n            </el-form-item>\n          </el-col>\n          <el-col :span=\"8\">\n            <el-form-item label=\"送样员:\">\n              <el-input v-model=\"sampleForm.sendUser\" style=\"width:95%\" disabled />\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row>\n          <el-col :span=\"24\">\n            <el-form-item label=\"采样备注:\">\n              <el-input v-model=\"sampleForm.collectRemark\" type=\"textarea\" :rows=\"3\" maxlength=\"50\" show-word-limit style=\"width:98%\" disabled />\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-divider>库存信息</el-divider>\n        <el-row>\n          <el-col :span=\"6\">\n            <el-form-item label=\"库存位置:\">\n              <el-input v-model=\"sampleForm.storeLocation\" style=\"width:95%\" disabled />\n            </el-form-item>\n          </el-col>\n          <el-col :span=\"6\">\n            <el-form-item label=\"入库时间:\">\n              <el-input v-model=\"sampleForm.storeTime\" style=\"width:95%\" disabled />\n            </el-form-item>\n          </el-col>\n          <el-col :span=\"6\">\n            <el-form-item label=\"可领次数:\">\n              <el-input v-model=\"sampleForm.avalDrawCount\" style=\"width:95%\" disabled />\n            </el-form-item>\n          </el-col>\n          <el-col :span=\"6\">\n            <el-form-item label=\"已领次数:\">\n              <el-input v-model=\"sampleForm.drawCount\" style=\"width:95%\" disabled />\n            </el-form-item>\n          </el-col>\n        </el-row>\n      </el-form>\n      <div slot=\"footer\" class=\"dialog-footer\">\n        <el-button type=\"primary\" @click=\"sampleVisible = false\">关 闭</el-button>\n      </div>\n    </el-dialog>\n\n  </div>\n</template>\n\n<script>\nimport * as stockServer from '@/api/stock'\nimport Pagination from '@/components/Pagination' // Secondary package based on el-pagination\nimport qs from 'qs'\nconst ACCOUNT_STATUS = {\n  '0': '草稿',\n  '1': '待审核',\n  '2': '审核驳回',\n  '3': '审核通过',\n  '4': '审核通过',\n  '5': '审核通过',\n  '6': '审核通过'\n}\nconst CHECK_STATUS = {\n  '0': '单次',\n  '1': '周',\n  '2': '月',\n  '3': '季度',\n  '4': '半年',\n  '5': '年'\n}\nexport default {\n  components: { Pagination },\n  filters: {\n    statusQualityFilter(status) {\n      const statusMap = {\n        '0': 'info',\n        '1': '',\n        '2': 'danger',\n        '3': 'success'\n      }\n      return statusMap[status]\n    }\n  },\n  data() {\n    return {\n      ACCOUNT_STATUS,\n      CHECK_STATUS,\n      stockData: [],\n      searchForm: {\n        jobId: '',\n        sampItemId: '',\n        pageNumber: 1,\n        pageSize: 10\n      },\n      total: 0,\n      approveVisible: false,\n      sampleVisible: false,\n      sampleForm: {\n        collectUser: '', // 采样员\n        sampleProperties: '',\n        sampleFixative: '',\n        collectRemark: '',\n        collectLocation: '',\n        collectIamge1: '',\n        collectIamge2: '',\n        collectIamge3: '',\n        collectIamge4: '',\n        factorName: ''\n      },\n      currentSample: '',\n      approveForm: {\n        sampItemId: '',\n        auditFlag: '1',\n        auditReason: ''\n      },\n      sampleImage: '',\n      sampleImageList: [],\n      factorVisible: false,\n      outStockLoading: false,\n      multipleSelection: []\n    }\n  },\n  created() {\n    this.outStockLoading = false\n    this.handleSearch()\n  },\n  methods: {\n    handleSearch() {\n      stockServer.getSampDrawApplyList(this.searchForm).then(res => {\n        if (res.code === 200) {\n          this.stockData = res.result.records\n          this.total = res.result.total\n        }\n      })\n    },\n    handleDetail(row) {\n      // 获取数据\n      this.currentSample = row.sampleNo\n      stockServer.getSampleDetailBySampItemId(row.sampItemId).then(res => {\n        if (res.code === 200) {\n          this.sampleForm = res.result || {}\n        }\n        this.sampleVisible = true\n      })\n    },\n    handleBatchOutStock() {\n      if (this.multipleSelection && this.multipleSelection.length > 0) {\n        const drawApplyIds = this.multipleSelection.map(i => {\n          return i.drawApplyId\n        })\n        const obj = {\n          drawApplyId: drawApplyIds.toString()\n        }\n        this.handleOutStock(obj)\n      } else {\n        this.$message.warning('至少选择一条记录')\n      }\n    },\n\n    handleSelectionChange(val) {\n      this.multipleSelection = val\n    },\n    handleOutStock(row) {\n      this.$confirm('确定出库?', '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).then(() => {\n        this.outStockLoading = true\n        stockServer.doDrawSample(qs.stringify({ drawApplyId: row.drawApplyId })).then(result => {\n          this.outStockLoading = false\n          if (result.code === 200) {\n            this.$notify({\n              title: '成功',\n              message: result.message,\n              type: 'success'\n            })\n            this.handleSearch()\n          } else {\n            this.$notify({\n              title: '失败',\n              message: result.message,\n              type: 'error'\n            })\n          }\n        })\n      }).catch(() => {\n        this.outStockLoading = false\n      })\n    }\n  }\n}\n</script>\n\n<style scoped lang=\"scss\" type=\"text/scss\">\n  .search_div {\n    >div {\n      &:nth-child(1){\n        margin-right: 10px;\n      }\n      display: inline-block;\n    }\n  }\n\n  .search_input {\n    width: 200px;\n    margin-bottom: 5px;\n  }\n  .search-right {\n    text-align: right;\n    padding-right: 25px;\n  }\n  .item {\n    font-size: 16px;\n  }\n  ::v-deep .el-card .el-card__header {\n    padding: 5px 20px;\n    background: #F3F2F2;\n  }\n</style>\n\n<style scoped 