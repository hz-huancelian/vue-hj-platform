 (status === '5') {\n        return 'success'\n      } else {\n        return 'danger'\n      }\n    }\n  },\n  data() {\n    return {\n      CHECK_STATUS,\n      checkData: [],\n      searchForm: {\n        smapItemId: '',\n        checkStatus: '4',\n        pageNumber: 1,\n        pageSize: 10\n      },\n      total: 0,\n      detailVisible: false,\n      sampleForm: {\n        factorInfo: {},\n        subFactors: {}\n      },\n      approveForm: {\n        checkFactorId: '',\n        auditFlag: '1',\n        auditReason: ''\n      },\n      approveVisible: false,\n      subSampleData: [],\n      batchApproveVisible: false,\n      batchApproveForm: {\n        checkFactorId: '',\n        auditFlag: '1',\n        auditReason: ''\n      },\n      multipleSelection: []\n    }\n  },\n  created() {\n    this.handleSearch()\n  },\n  methods: {\n    handleSearch() {\n      checkServer.findCheckFactorByCondition(this.searchForm).then(res => {\n        if (res.code === 200) {\n          this.checkData = res.result.records.map(i => {\n            if (i.checkRes && i.checkRes !== '') {\n              const ob = JSON.parse(i.checkRes)\n              this.$set(i, 'masterRes', ob.v1 ? ob.v1 : 0)\n              this.$set(i, 'exponRes', ob.v2 ? ob.v2 : 0)\n            } else {\n              this.$set(i, 'masterRes', 0)\n              this.$set(i, 'exponRes', 0)\n            }\n            return i\n          }) || []\n          this.total = res.result.total\n        }\n      })\n    },\n    handleDetail(row) {\n      this.sampleForm = { ...row }\n      if (row.factorSubsetVos && row.factorSubsetVos.length > 0) {\n        this.sampleForm.factorSubsetVos.forEach(i => {\n          const factorInfo = JSON.parse(i.checkSubRes)\n          this.$set(i, 'factorInfo', factorInfo)\n        })\n      }\n      if (row.checkRes && row.checkRes.length > 0) {\n        const factorInfo = JSON.parse(row.checkRes)\n        this.$set(this.sampleForm, 'factorInfo', factorInfo)\n      }\n      this.detailVisible = true\n    },\n    handleApprove(row) {\n      this.approveForm.checkFactorId = row.checkFactorId\n      this.approveForm.auditFlag = '1'\n      this.approveForm.auditReason = ''\n      this.approveVisible = true\n    },\n    batchConfirmDialog() {\n      this.$confirm('确定提交?', '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).then(() => {\n        checkServer.batchAuditCheckFactor(this.batchApproveForm).then(res => {\n          if (res.code === 200) {\n            this.$notify({\n              title: '成功',\n              message: res.message,\n              type: 'success'\n            })\n            this.batchApproveVisible = false\n            this.handleSearch()\n          } else {\n            this.$notify({\n              title: '失败',\n              message: res.message,\n              type: 'error'\n            })\n          }\n        })\n      }).catch(() => {})\n    },\n    handleSelectionChange(val) {\n      this.multipleSelection = val\n    },\n    handleBatchApprove() {\n      if (this.multipleSelection && this.multipleSelection.length > 0) {\n        const checkFactors = this.multipleSelection.map(i => {\n          return i.checkFactorId\n        })\n        debugger\n        this.batchApproveForm.checkFactorId = checkFactors.toString()\n        this.batchApproveForm.auditFlag = '1'\n        this.batchApproveForm.auditReason = ''\n        this.batchApproveVisible = true\n      } else {\n        this.$message.warning('至少选择一条记录进行审核')\n      }\n    },\n    confirmDialog() {\n      this.$confirm('确定提交?', '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).then(() => {\n        checkServer.auditCheckFactor(this.approveForm).then(res => {\n          if (res.code === 200) {\n            this.$notify({\n              title: '成功',\n              message: res.message,\n              type: 'success'\n            })\n            this